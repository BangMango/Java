			count+=1;
			return 10000;
		}else if(money<10000 && money>=5000) {
			count+=1;
			return 5000;
		}else if(money<5000 && money>=1000) {
			count+=1;
			return 1000;
		}else if(money<1000 && money>=500) {
			count+=1;
			return 500;
		}else if(money <500 && money>=100){
			count+=1;
			return 100;
