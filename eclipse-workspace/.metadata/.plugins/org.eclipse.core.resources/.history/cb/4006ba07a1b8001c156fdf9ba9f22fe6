package enumExample;

public class Main {

	public static void main(String[] args) {
		String shapeType="CIRCLE";
		if(shapeType=="CIRCLE") {
			System.out.println("동그라미");
		}else if(shapeType=="TRIANGLE") {
			System.out.println("삼각형");
		}else if(shapeType=="SQUARE") {
			System.out.println("사각형");
		}else {
			
		}

	}

}
